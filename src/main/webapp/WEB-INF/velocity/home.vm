<html>
	<head><title>Cinema</title></head>
	<body>
		<h1>Welcom to Home Page Cinema Web Application!!!</h1>
		
      <form class="form-horizontal" enctype="multipart/form-data" action="${rc.contextPath}/upload" method="post">
          <div class="form-actions">
            <h2>Select users.json file</h2>
            <input type="file" name="users"/>
            <h2>Select accounts.json file</h2>
            <input type="file" name="accounts"/>
            <h2>Select events.json file</h2>
            <input type="file" name="events"/>
            <hr>
            <button class="btn btn-primary" type="submit">Upload</button>
          </div>
      </form>
		<div>
			<a href="${rc.contextPath}/accounts">GET:/accounts</a>
			<br>
			<a href="${rc.contextPath}/users">GET:/users</a>
			<br>
			<a href="${rc.contextPath}/users/2">GET:/users/2</a>
			<br>
			<a href="${rc.contextPath}/users/2/tickets">GET:/users/2/tickets</a>
			<br>
			<a href="${rc.contextPath}/users/2/tickets.pdf">GET:/users/2/tickets.pdf</a>
			<br>
			<a href="${rc.contextPath}/events">GET:/events</a>
			<br>
			<a href="${rc.contextPath}/events/1">GET:/events/1</a>
			<br>
			<a href="${rc.contextPath}/events/1/tickets">GET:/events/1/tickets</a>
			<br>
			<a href="${rc.contextPath}/events/1/tickets.pdf">GET:/events/1/tickets.pdf</a>
			<br>
			<a href="${rc.contextPath}/tickets/events/1">GET:/tickets/events/1</a>
			<br>
			<a href="${rc.contextPath}/tickets/events/1.pdf">GET:/tickets/events/1.pdf</a>
			<br>
			<a href="${rc.contextPath}/tickets/book">POST:/tickets/book</a>
			<br>
		</div>
		<a href="${rc.contextPath}/logout"><h1>LOGOUT</h1></a>
	</body>
</html>